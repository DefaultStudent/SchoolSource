<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.schsource.users.vo.Users" table="Users" schema="SchoolSource">
        <id name="usersId" column="UsersID"/>
        <property name="upwd" column="UPWD"/>
        <property name="uname" column="UNAME"/>
        <property name="ugender" column="UGENDER"/>
        <property name="ulimit" column="ULIMIT"/>
    </class>
</hibernate-mapping>