<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.schsource.school.vo.School" table="SCHOOL" schema="SchoolSource">
        <id name="schId" column="SchID"/>
        <property name="schName" column="SchNAME"/>
        <property name="schPic" column="SchPic"/>
        <property name="pid" column="PID"/>
        <property name="tid" column="TID"/>
        <property name="schAddress" column="SchADDRESS"/>
        <property name="teacher" column="TEACHER"/>
        <property name="tel" column="TEL"/>
    </class>
</hibernate-mapping>